<section class="is-range-slider">
    <span class="rangeValues"><output>${sliderOutput}</output></span>
    <input type="range"
           class="slider-a"
           @change="handleChange"
           v-model="valueSliderA"
           :min="minSliderValue"
           :max="maxSliderValue"
           :step="sliderStep" />
    <input type="range"
           class="slider-b"
           @change="handleChange"
           v-model="valueSliderB"
           :min="minSliderValue"
           :max="maxSliderValue"
           :step="sliderStep" />
</section>
