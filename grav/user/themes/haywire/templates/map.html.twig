{% extends "partials/base.html.twig" %}

{% set propertyAggregateTemplate = 'components/property-aggregate.html.twig' %}

{% block head %}
    {{ parent() }}
    <link rel="stylesheet" href="https://js.arcgis.com/4.4/esri/css/main.css">
{% endblock %}

{% block content %}
    <page-state inline-template>
        <property-aggregate
            :parent-event-bus="eventBus"
            :url-params="urlParams"
            inline-template>
            {% include propertyAggregateTemplate with {} only %}
        </property-aggregate>
    </page-state>
{% endblock %}

{% block bottom %}
    {{ parent() }}
{% endblock %}
